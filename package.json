{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^4.25.7",
    "gatsby-cli": "^2.6.2",
    "gatsby-image": "^2.1.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.1",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-source-graphql": "^2.0.18",
    "gatsby-transformer-sharp": "^2.1.19",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "graphql": "^14.3.1",
    "netlify-cli": "^2.11.19",
    "netlify-lambda": "^1.4.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && run-p build:*",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/functions",
    "clean": "run-p clean:*",
    "clean:cache": "rimraf .cache",
    "clean:public": "rimraf public",
    "clean:functions": "rimraf functions",
    "deploy": "npm run netlify deploy",
    "dev": "npm run clean && npm run start",
    "format": "prettier --write src/**/*.{js,jsx}",
    "netlify": "netlify",
    "start": "run-p start:*",
    "start:netlifyDev": "netlify dev",
    "start:netlifyDev:command": "gatsby develop",
    "start:lambda": "netlify-lambda serve src/functions",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
